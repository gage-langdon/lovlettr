{
  "name": "LoveLttr",
  "private": true,
  "description": "Modern love letters built with Gatsby",
  "version": "0.1.0",
  "author": "Gage Langdon and Maxton Delaney",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest ./src"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "css"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "gatsby": "^2.1.31",
    "gatsby-image": "^2.0.33",
    "gatsby-plugin-create-client-paths": "^2.0.5",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.9",
    "gatsby-plugin-sharp": "^2.0.28",
    "gatsby-source-filesystem": "^2.0.24",
    "gatsby-transformer-sharp": "^2.1.17",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-quill": "^1.3.3"
  },
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jsdom": "^14.0.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.4"
  }
}
